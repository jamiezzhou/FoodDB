<body>
  <section id="title">
    <div>
      <div class="header">
        <a
          class="direct-link"
          href="/Users/yutingliu/Desktop/cs3265-food-web/account.html"
          >Create Account</a
        >
        <a
          class="direct-link"
          href="/Users/yutingliu/Desktop/cs3265-food-web/delivery.html"
          >Delivery</a
        >
      </div>
    </div>
  </section>

  <!-- find restaurants section -->
  <div class="container">
    <div class="row">
      <div class="booking-cta">
        <h1>Find Restaurants</h1>
      </div>
      <div class="booking-form">
        <form>
          <div class="row">
            <div class="col-md-8">
              <div class="form-group">
                <span class="form-label">Address Or Name</span>
                <input
                  class="form-control"
                  type="text"
                  placeholder="Enter Name or Address of the Restaurant"
                />
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <span class="form-label">Types</span>
                <select class="form-control">
                  <option>Fine Dining</option>
                  <option>Convenience Food</option>
                  <option>Snacks&Drinks</option>
                </select>
                <span class="select-arrow"></span>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-4">
              <div class="form-group">
                <span class="form-label">Reservation Number</span>
                <input
                  class="form-control"
                  type="text"
                  placeholder="Enter Numbers Only"
                />
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <span class="form-label">Rating</span>
                <select class="form-control">
                  <option>1</option>
                  <option>2</option>
                  <option>3</option>
                  <option>4</option>
                  <option>5</option>
                </select>
                <span class="select-arrow"></span>
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-btn">
                <button class="submit-btn">Search</button>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>

  <!-- Reservation Section -->
  <div class="container">
    <div class="row">
      <div class="booking-cta">
        <h1>Make Your Reservation</h1>
      </div>
      <div class="booking-form">
        <form>
          <div class="row">
            <div class="col-md-8">
              <div class="form-group">
                <span class="form-label">Restaurant</span>
                <input
                  class="form-control"
                  type="text"
                  placeholder="Enter Name and Address of the Restaurant"
                />
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <span class="form-label">Guests</span>
                <select class="form-control">
                  <option>1</option>
                  <option>2</option>
                  <option>3</option>
                  <option>4</option>
                  <option>5</option>
                  <option>6</option>
                  <option>7</option>
                  <option>8</option>
                  <option>9</option>
                </select>
                <span class="select-arrow"></span>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-4">
              <div class="form-group">
                <span class="form-label">Special Requests</span>
                <input
                  class="form-control"
                  type="text"
                  placeholder="Enter Special Requests"
                />
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <span class="form-label">Date</span>
                <input class="form-control" type="date" required />
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-btn">
                <button class="submit-btn">Make Reservation</button>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>

  <!-- review section -->
  <div class="container">
    <div class="row">
      <!-- <div class="booking-cta">
        <h1>Reviews</h1>
      </div> -->
      <div class="reviews">
      <div class="booking-form">
        
        <form>
          <div class="row">
            <div class="col-md-4">
              <div class="form-group">
                <h2>Reviews</h2>
              </div>
              </div>
            <div class="col-md-4">
              <div class="form-group">
                <span class="form-label">filter</span>
                <select class="form-control">
                  <option>HIGH/LOW</option>
                  <option>LOW/HIGH</option>
                </select>
                <span class="select-arrow"></span>
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-btn">
                <button class="submit-btn">Search</button>
              </div>
            </div>
          </div>
          
        </form>
      </div>
      </div>
    </div>
  </div>
</body>
